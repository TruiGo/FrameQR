package com.alipay.android.appHall.component.incrementitem;

public class GetMoneyDataStructure {
    public GetMoneyDataStructure(boolean isContainSelf, int getMoneypeopleCount, double getMoneyTotalCount) {
        this.isContainSelf = isContainSelf;
        this.getMoneypeopleCount = getMoneypeopleCount;
        this.getMoneyTotalCount = getMoneyTotalCount;
    }

    public boolean isContainSelf;       //收款人是否包含自己
    public int     getMoneypeopleCount; //收款人数
    public double     getMoneyTotalCount;  //首款总金额
}
