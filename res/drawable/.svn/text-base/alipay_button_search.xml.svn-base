<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Non focused states -->
   
    <item android:state_focused="false" android:state_selected="false" android:state_pressed="false" android:state_enabled="true" android:drawable="@drawable/contact_n" />
    <item android:state_focused="false" android:state_selected="true" android:state_pressed="false" android:state_enabled="true" android:drawable="@drawable/contact_n" />
    
    <!-- Focused states -->
    
    <item android:state_focused="true" android:state_selected="false" android:state_pressed="false" android:state_enabled="true" android:drawable="@drawable/contact_s" />
    <item android:state_focused="true" android:state_selected="true" android:state_pressed="false" android:state_enabled="true" android:drawable="@drawable/contact_s" />
    
    <!-- Pressed -->
   
    <item android:state_pressed="true" android:state_enabled="true" android:drawable="@drawable/contact_s" />
    
    <!-- Disabled -->
   
    <item android:state_enabled="false" android:state_focused="false" android:state_selected="false" android:state_pressed="false" android:drawable="@drawable/contact_d" />
</selector>