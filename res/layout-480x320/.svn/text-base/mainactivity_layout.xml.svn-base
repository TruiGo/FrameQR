        </FrameLayout>

        <ImageView
            android:id="@+id/overshadow3"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/item_Coupon"
            android:layout_marginTop="-10dp"
            android:background="@+drawable/cardshadow" />

        <ImageView
            android:id="@+id/overlay3"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/overshadow3"
            android:background="@drawable/home_item_overlay" />
        
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/overshadow3"
            android:layout_alignLeft="@+id/overlay3"
            android:background="@drawable/overlayshadowleft" />
        
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/overshadow3"
            android:layout_alignRight="@+id/overlay3"
            android:background="@drawable/overlayshadowright" />

        <LinearLayout
            android:id="@+id/appsShortcut"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/item_Coupon"
            android:layout_marginTop="14dp"
            android:paddingLeft="12dp"
            android:paddingRight="12dp"
            android:orientation="horizontal" >

            <include
                android:id="@+id/appFirst"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="0dp"
                android:layout_weight="1"
                layout="@layout/subitem_layout_homepage" >
            </include>

            <include
                android:id="@+id/appSecond"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                layout="@layout/subitem_layout_homepage" >
            </include>

            <include
                android:id="@+id/appThird"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                layout="@layout/subitem_layout_homepage" >
            </include>

            <include
                android:id="@+id/appFourth"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginRight="0dp"
                layout="@layout/subitem_layout_homepage" >
            </include>
        </LinearLayout>

        <ImageView
            android:id="@+id/overshadow4"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/appsShortcut"
            android:layout_marginTop="-40dp"
            android:background="@drawable/cardshadow" />

        <ImageView
            android:id="@+id/overlay4"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/overshadow4"
            android:background="@drawable/home_item_overlay" />
        
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/overshadow4"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="16dp"
            android:background="@drawable/homepage_alipay_logo_320" />
        
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/overshadow4"
            android:layout_alignLeft="@+id/overlay4"
            android:background="@drawable/overlayshadowleft" />
        
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/overshadow4"
            android:layout_alignRight="@+id/overlay4"
            android:background="@drawable/overlayshadowright" />

        <LinearLayout
            android:id="@+id/Placeholder"
            android:layout_width="fill_parent"
            android:layout_height="0dp"
            android:layout_below="@+id/appsShortcut"
            android:layout_weight="1"
            android:src="@drawable/application_bg" >
        </LinearLayout>

        <LinearLayout
            android:id="@+id/bgbottom"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:background="@drawable/bottomshadow" >
        </LinearLayout>
    </RelativeLayout>

    <com.alipay.android.ui.widget.SlidingDrawer
        android:id="@+id/appslidingDrawer"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_margin="0dp"
        component:content="@+id/apps_layout"
        component:handle="@+id/handle" >

        <FrameLayout android:layout_width="fill_parent" android:layout_height="fill_parent" android:id="@+id/apps_layout" android:background="@drawable/light_bg">
            
            
        <GridView
            android:id="@+id/apps_grid"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            
            android:fadingEdgeLength="0dp"
            android:focusableInTouchMode="true"
            android:listSelector="@android:color/transparent"
            android:numColumns="4"
            android:paddingTop="10dp"
            android:scrollbars="vertical" />
            
        </FrameLayout>

        <Button
            android:id="@+id/handle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/appCenter"
            android:background="@drawable/slidingdrawer_bg_bottom"
            android:textSize="24dip" />
    </com.alipay.android.ui.widget.SlidingDrawer>
    <!-- 新功能介绍首页向导页面 -->
	<com.alipay.android.comon.component.CommonGallery
		android:layout_width="fill_parent" android:layout_height="fill_parent"
		android:id="@+id/guideGallery" android:fadingEdgeLength="0dp"
		android:spacing="0dp" android:unselectedAlpha="1.0" android:visibility="gone"
		android:alwaysDrawnWithCache="false"/>
</com.alipay.android.appHall.uiengine.CacheFrameLayout>