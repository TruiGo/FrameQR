                    <TextView android:id="@+id/voucher_status" android:textColor="#60a02a" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="可使用" android:singleLine="true" android:ellipsize="end" android:layout_gravity="right"/>
                    <LinearLayout android:id="@+id/voucherTime_layout" android:layout_width="wrap_content" android:layout_height="wrap_content" android:orientation="horizontal" android:layout_gravity="right">
